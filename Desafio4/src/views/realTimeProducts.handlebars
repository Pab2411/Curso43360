<div>
    <h1>Lista de Productos</h1>

    <form onsubmit="addProduct(event)">
        <input type="text" name="title" placeholder="Nombre del producto" required>
        <input type="text" name="description" placeholder="DescripciÃ³n del producto" required>
        <input type="text" name="code" placeholder="Codigo del producto" required>
        <input type="number" name="price" placeholder="Precio del producto" required>
        <input type="number" name="stock" placeholder="Stock producto" required>
        <input type="text" name="category" placeholder="Categoria del producto" required>
        <input type="" name="thumbnails" placeholder="imagen">


        <button type="submit">Agregar producto</button>
    </form>
    <h1>Productos en tiempo real</h1>
    <ul id="productList">
        {{#each products}}
        <li>{{this.title}}</li>
        {{/each}}
    </ul>
</div>

